{
  "name": "frolox/bga_ldh",
  "type": "project",
  "description": "Lignee des Heros BoardGame for BGA",
  "license": "MIT",
  "require": {
    "php": "^7.4.0",
    "ext-SimpleXML": "*",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "symfony/config": "^5.4",
    "symfony/dependency-injection": "^5.4",
    "symfony/finder": "^5.4",
    "symfony/yaml": "^5.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "LdH\\": "modules/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LdH\\Tests\\": "modules/Tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
